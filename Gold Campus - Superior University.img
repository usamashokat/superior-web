        .ipe-table th{background:#f8fafc;text-align:left;white-space:nowrap}

        /* By default, wrap text in td cells */
        .ipe-table td{white-space:normal;overflow-wrap:anywhere;word-break:break-word}

        /* Optional: tighter wrap for long meta fields */
        .ipe-wrap{white-space:normal;overflow-wrap:anywhere;word-break:break-word}

        /* MOBILE — stacked "card" rows */
        @media (max-width: 480px){
