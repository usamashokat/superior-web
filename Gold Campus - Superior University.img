    <script id="ipe-inline-js" type="text/javascript">
        (function($){
            if (typeof ipe_data === 'undefined') return;

            var $continent = $('#ipe-continent');
            var $country   = $('#ipe-country');
            var $search    = $('#ipe-search');

            function populateContinents() {
                // Prevent double-filling if something triggers twice
                if ($continent.data('ipe-filled')) return;

                $continent.empty().append($('<option/>',{
                    value:'',
                    text:'Select Continent'
                }));

                var continents = ipe_data.continents || {};
                var seenByName = Object.create(null);

                Object.keys(continents).forEach(function(id){
                    var name = (continents[id] || '').trim();
                    if (!name) return;
                    var key = name.toLowerCase();
                    if (seenByName[key]) return; // de-dup by name
                    seenByName[key] = true;
                    $continent.append($('<option/>',{value:id,text:name}));
                });

                $continent.data('ipe-filled', true);
            }

            function populateCountries(parentId) {
                $country.empty().append($('<option/>',{value:'',text: 'Select Country'}));
                if (!parentId) return;
                var map = ipe_data.countries || {};
                var list = map[parentId] || [];
                list.sort(function(a,b){return (a.name||'').localeCompare(b.name||'');});
                var used = {};
                list.forEach(function(c){
                    if (!c || !c.id) return;
                    if (used[c.id]) return; used[c.id]=true;
                    $country.append($('<option/>',{value:c.id,text:c.name}));
                });
            }

            function ipeAjaxFilter(page) {
                page = page || 1;
                $.post(ipe_data.ajax_url, {
                    action: 'ipe_ajax_filter_posts',
                    nonce: ipe_data.nonce,
                    continent: $continent.val(),
                    location_term: $country.val(),
                    s: $search.val(),
                    page: page
                }, function(resp){
                    if (resp && resp.success) {
                        $('#ipe-partnerships-table tbody').html(resp.data.table_body);
                        $('#ipe-pagination').html(resp.data.pagination);
                    }
                });
            }

            // Events
            $(document).on('change', '#ipe-continent', function(){
                populateCountries($(this).val());
                ipeAjaxFilter(1);
            });
            $(document).on('change', '#ipe-country', function(){
                ipeAjaxFilter(1);
            });

            var timer = null;
            $(document).on('input', '#ipe-search', function(){
                clearTimeout(timer);
                timer = setTimeout(function(){ ipeAjaxFilter(1); }, 300);
            });

            // AJAX pagination
            $(document).on('click', '#ipe-pagination a.page-numbers', function(e){
                e.preventDefault();
                var href = $(this).attr('href') || '';
                var page = 1, m = href.match(/[?&]paged=(\d+)/);
                if (m) page = parseInt(m[1],10)||1;
                else {
                    var n = parseInt($(this).text(),10);
                    if (!isNaN(n)) page = n;
                }
                ipeAjaxFilter(page);
            });

            // Init once per page render
            populateContinents();
        })(jQuery);
