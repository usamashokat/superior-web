			function formatCNIC(raw) {
				// Keep digits only and limit to 13
				var digits = String(raw).replace(/\D/g, '').slice(0, 13);

				var part1 = digits.substring(0, 5);   // first 5
				var part2 = digits.substring(5, 12);  // next 7
				var part3 = digits.substring(12, 13); // last 1

				var out = '';
				if (part1) { out += part1; }
				if (part2) { out += '-' + part2; }
				if (part3) { out += '-' + part3; }

				return out;
			}

			// Any Forminator field wrapper with class "cnic-field"
			$(document).on('input', '.cnic-field input', function () {
				this.value = formatCNIC(this.value);
			});

		});
	})(jQuery);
